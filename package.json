{
  "name": "node-extended",
  "private": false,
  "version": "0.1.0",
  "description": "A set of helper functions for node",
  "main": "./dist/node-extended.js",
  "scripts": {
    "prepublishOnly": "npm run build && npm run test && npm run generate-docs",
    "generate-docs": "md-magic --path 'README.md' --config ./markdown.config.js",
    "build": "tsc",
    "start": "tsc --watch",
    "test": "jest"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  },
  "author": "Robbie Cook",
  "license": "ISC",
  "dependencies": {
    "child_process": "^1.0.2",
    "markdown-magic": "^1.0.0",
    "node-fetch": "^2.6.0",
    "node-notifier": "^9.0.0",
    "process": "^0.11.10",
    "readline": "^1.3.0",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.3.1",
    "util": "^0.12.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.5",
    "@semantic-release/npm": "^7.0.9",
    "@types/jest": "^26.0.4",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "jest": "^26.1.0",
    "jsdoc-to-markdown": "^7.0.1",
    "semantic-release": "^17.3.3",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robbie-Cook/node-extended.git"
  },
  "bugs": {
    "url": "https://github.com/Robbie-Cook/node-extended/issues"
  },
  "homepage": "https://github.com/Robbie-Cook/node-extended#readme"
}
